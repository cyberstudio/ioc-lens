{
    "name": "ioc-lens",
    "version": "1.0.0",
    "scripts": {
        "postinstall": "lefthook install",
        "start": "cross-env NODE_OPTIONS=--openssl-legacy-provider webpack --watch --config webpack/extension/webpack.dev.js",
        "start:ui-book": "cross-env NODE_OPTIONS=--openssl-legacy-provider webpack serve --config webpack/ui-book/webpack.config.js",
        "build": "cross-env NODE_OPTIONS=--openssl-legacy-provider webpack --config webpack/extension/webpack.prod.js",
        "build:tokens": "cross-env NODE_OPTIONS=--openssl-legacy-provider node ./tools/tokens-builder",
        "lint:ts": "cross-env NODE_OPTIONS=--openssl-legacy-provider eslint ./src/**/*.ts",
        "lint:styles": "cross-env NODE_OPTIONS=--openssl-legacy-provider stylelint ./src/**/*.css",
        "test": "cross-env NODE_OPTIONS=--openssl-legacy-provider TS_NODE_PROJECT='./tsconfig.spec.json' uvu -r ts-node/register"
    },
    "devDependencies": {
        "@koobiq/design-tokens": "3.0.0-beta.7",
        "@namics/stylelint-bem": "9.0.0",
        "@types/chrome": "0.0.250",
        "@types/lodash": "4.14.200",
        "@typescript-eslint/eslint-plugin": "6.9.0",
        "@typescript-eslint/parser": "6.9.0",
        "copy-webpack-plugin": "11.0.0",
        "cross-env": "7.0.3",
        "css-loader": "6.8.1",
        "css-minimizer-webpack-plugin": "5.0.1",
        "eslint": "8.52.0",
        "eslint-config-prettier": "9.0.0",
        "eslint-plugin-prettier": "5.0.1",
        "html-webpack-plugin": "5.5.0",
        "lefthook": "1.5.2",
        "lodash": "4.17.20",
        "mini-css-extract-plugin": "2.7.6",
        "postcss-loader": "7.3.3",
        "postcss-nesting": "12.0.1",
        "prettier": "3.0.3",
        "style-dictionary": "3.7.1",
        "stylelint": "15.11.0",
        "stylelint-config-standard": "34.0.0",
        "ts-loader": "9.5.0",
        "ts-node": "10.9.1",
        "typescript": "5.2.2",
        "uvu": "0.5.6",
        "webpack": "5.89.0",
        "webpack-cli": "5.1.4",
        "webpack-dev-server": "4.15.1",
        "webpack-merge": "5.10.0"
    }
}
